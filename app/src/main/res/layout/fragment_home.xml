<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="254dp"
        android:layout_height="105dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="604dp"
        android:adjustViewBounds="true"
        android:contentDescription="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/a"
        android:visibility="visible" />
    <!-- 빽다방 이미지 버튼-->
    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="120dp"
        android:maxWidth="35dp"
        android:maxHeight="35dp"
        android:padding="0sp"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />
    <!--스타벅스 이미지 버튼-->
    <ImageButton
        android:id="@+id/starbucksButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="120dp"
        android:maxWidth="35dp"
        android:maxHeight="35dp"
        android:padding="0sp"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/backButton"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/starbucks" />
    <!--이디야 이미지 버튼-->
    <ImageButton
        android:id="@+id/ediyaButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="120dp"
        android:maxWidth="35dp"
        android:maxHeight="35dp"
        android:padding="0sp"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/starbucksButton"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ediya" />
    <!--컴포즈 이미지 버튼-->
    <ImageButton
        android:id="@+id/composeButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="290dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="20dp"
        android:maxWidth="35dp"
        android:maxHeight="35dp"
        android:padding="0sp"
        android:scaleType="centerCrop"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ediyaButton"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/compose" />
    <!--엔젤리너스 이미지 버튼-->
    <ImageButton
        android:id="@+id/angelButton"
        android:layout_width="60dp"
        android:layout_height="60dp"

        android:layout_marginStart="50dp"
        android:layout_marginTop="220dp"
        android:padding="0sp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"
        app:srcCompat="@drawable/angel" />
    <!--할리스이미지 버튼-->
    <ImageButton
        android:id="@+id/hollysButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="220dp"
        android:padding="0sp"
        android:scaleType="centerCrop"
        android:background="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/angelButton"
        app:layout_constraintTop_toBottomOf="@+id/starbucksButton"
        app:srcCompat="@drawable/hollus" />
    <!--메가커피 이미지 버튼-->
    <ImageButton
        android:id="@+id/megaButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="220dp"
        android:padding="0sp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/hollysButton"
        app:layout_constraintTop_toBottomOf="@+id/ediyaButton"
        app:srcCompat="@drawable/mega" />
    <!--파스구찌 이미지 버튼-->
    <ImageButton
        android:id="@+id/ppascucciButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="290dp"
        android:layout_marginTop="220dp"
        android:padding="0sp"
        android:scaleType="centerCrop"
        android:background="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/megaButton"
        app:layout_constraintTop_toBottomOf="@+id/composeButton"
        app:srcCompat="@drawable/pascucci" />
    <!--탐앤탐스 이미지 버튼-->
    <ImageButton
        android:id="@+id/tomButton"
        android:layout_width="56dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="320dp"
        android:padding="0sp"
        android:scaleType="centerCrop"
        android:background="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/twsomeButton"
        app:layout_constraintTop_toBottomOf="@+id/hollysButton"
        app:srcCompat="@drawable/tomntoms" />
    <!--투썸플레이스 이미지 버튼-->
    <ImageButton
        android:id="@+id/twsomeButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="320dp"
        android:padding="0sp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/angelButton"
        app:srcCompat="@drawable/twosome" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="180dp"
        android:fontFamily="@font/jalnan"
        android:text="백다방"

        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="137dp"
        android:layout_marginTop="180dp"
        android:fontFamily="@font/jalnan"
        android:text="스타벅스"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"

        />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="220dp"
        android:layout_marginTop="180dp"
        android:fontFamily="@font/jalnan"
        android:text="이디야"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"

        />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="300dp"
        android:layout_marginTop="180dp"
        android:fontFamily="@font/jalnan"
        android:text="컴포즈"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"

        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="53dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/jalnan"
        android:text="엔젤리너스"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"

        />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="145dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/jalnan"
        android:text="할리스"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"

        />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="220dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/jalnan"
        android:text="메가커피"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"

        />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="297dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/jalnan"
        android:text="파스쿠찌"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="57dp"
        android:layout_marginTop="380dp"
        android:fontFamily="@font/jalnan"
        android:text="탐앤탐스"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="127dp"
        android:layout_marginTop="380dp"
        android:fontFamily="@font/jalnan"
        android:text="투썸플레이스"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton" />

    <View
        android:id="@+id/divider"
        android:layout_width="260dp"
        android:layout_height="2dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="20dp"
        android:background="#808080" />

    <View
        android:id="@+id/divider2"
        android:layout_width="160dp"
        android:layout_height="2dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="430dp"
        android:layout_marginBottom="20dp"
        android:background="#00AAFF" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="85dp"
        android:fontFamily="@font/jalnan"
        android:text="브랜드"
        android:textColor="#808080"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="415dp"
        android:fontFamily="@font/jalnan"
        android:text="이 메뉴는 어때?"
        android:textColor="#00AAFF"
        android:textSize="20sp" />
    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="450dp"
        android:layout_marginEnd="30dp"
        android:id="@+id/rvRand"
        android:visibility="visible"/>

</FrameLayout>